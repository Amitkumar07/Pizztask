
  <%= form_for :search ,:class=>"form-horizontal", :url=>'/admin/search' do |f| %>
      <div class="form-group">
        <label for="inputEmail3" class="col-sm-2 control-label">Franchise:</label>
        <div class="col-sm-10">
          <%= f.collection_select(:franchise_id , Franchise.all, :id, :name, :prompt => 'Select', :class=>"form-control")  %>
        </div>
      </div>
      <div class="form-group">
        <label for="inputEmail3" class="col-sm-2 control-label">Pizza Type</label>
        <div class="col-sm-10">
          <%= f.collection_select(:pizza_type_id , PizzaType.all, :id, :name, :prompt => 'Select' , :class=>"form-control")  %>

        </div>
      </div>
      </div>
      <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
          <button type="submit" class="btn btn-default">Search</button>
        </div>
      </div>

  <% end %>
